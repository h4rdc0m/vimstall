set nocompatible
filetype on
filetype plugin on
filetype indent on
syntax on
set cursorline
set cursorcolumn
set relativenumber
set tabstop=4
set shiftwidth=4
set showmode
set hlsearch
set nowrap
set expandtab
set wildmenu
set wildmode=list:longest
set wildignore=*.docx,*.jpeg,*.png,*.gif,*.pdf,*.pyc,*.exe,*.flv,*.img,*.xlsx,*.mp4
colorscheme molokai
